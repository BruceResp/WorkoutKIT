{     "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${fileDirname}",
            "executable": "${workspaceRoot}/application/bin/application.elf",
            "name": "Debug with PyOCD",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "showDevDebugOutput": "raw",
            "servertype": "pyocd",
            "targetId": "STM32F103C8",
            "svdFile": "${workspaceRoot}/STM32F103C8Tx_VSCODE_IDE/STM32F103.svd",
            "serverpath": "D:/Tools/Python/Python310/Scripts/pyocd.exe",
            "preLaunchTask": "build_bat",
            "armToolchainPath": "D:/Tools/Complier_Toolchain/gcc-arm-none-eabi-10.3-2021.10/bin",
            //"gdbPath": "D:/Tools/Complier_Toolchain/gcc-arm-none-eabi-10.3-2021.10/bin/arm-none-eabi-gdb.exe",
            "cpu": "cortex-m3",
            //"debugServer": 4711,
            "swoConfig": {
                "enabled": true,
                "cpuFrequency": 72000000,
                "swoFrequency": 2000000,
                "source": "probe",
                "decoders": [
                    {
                        "port": 0,
                        "type": "console",
                        "label": "SWO output",
                        "encoding": "ascii"
                    }
                ]
            }
        },
        {
            "name": "Debug with OpenOCD",
            "cwd": "${fileDirname}",
            "device": "STM32F103C8T6",
            "executable": "${workspaceRoot}/application/bin/application.elf",
            "request": "launch",
            "type": "cortex-debug",
            "svdFile": "${workspaceRoot}/STM32F103C8Tx_VSCODE_IDE/STM32F103.svd",
            "runToEntryPoint": "main",
            //"gdbPath": "D:/Tools/Complier_Toolchain/gcc-arm-none-eabi-10.3-2021.10/bin/arm-none-eabi-gdb.exe",
            "servertype": "openocd",
            "configFiles": [
                "${workspaceRoot}/stlink-v2.cfg",
                "${workspaceRoot}/stm32f1x.cfg"
            ],
            "armToolchainPath": "D:/Tools/Complier_Toolchain/gcc-arm-none-eabi-10.3-2021.10/bin",
            "swoConfig": {
                "enabled": true,
                "cpuFrequency": 72000000,
                "swoFrequency": 2000000,
                "source": "probe",
                "decoders": [
                    {
                        "port": 0,
                        "type": "console",
                        "label": "SWO output",
                        "encoding": "ascii"
                    }
                ]
            }
        },
        
    ]
}